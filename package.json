{
  "name": "template-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint:script": "next lint",
    "lint:script:fix": "next lint --fix",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix '**/*.{css,scss,sass}'",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "storybook": "env-cmd -f .env.dev start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "export:dev": "env-cmd -f .env.dev next build && next export",
    "export:stg": "env-cmd -f .env.stg next build && next export",
    "export:prd": "env-cmd -f .env.prd next build && next export"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "gsap": "^3.11.3",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-scroll": "^1.8.7",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.5",
    "ress": "^5.0.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/react-scroll": "^1.8.3",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.8",
    "prettier": "2.6.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "4.6.3"
  }
}
